<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ac_repair_technicians_fragement"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AcRepairTechniciansFragment">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:text="Technicians Available nearby"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.123" />

    <RadioGroup
        android:id="@+id/technicianGroup"
        android:layout_width="165dp"
        android:layout_height="156dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355">

        <RadioButton
            android:id="@+id/technician1"
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:text="Technician 1" />

        <RadioButton
            android:id="@+id/technician2"
            android:layout_width="145dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:text="Technician 2" />

        <RadioButton
            android:id="@+id/technician3"
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:text="Technician 3" />


    </RadioGroup>

    <Button
        android:id="@+id/buttonRequest"
        android:layout_width="139dp"
        android:layout_height="46dp"
        android:onClick="onClickSubmitButton"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.554" />

    <TextView
        android:id="@+id/tnoTechnician"
        android:layout_width="143dp"
        android:layout_height="38dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />
</androidx.constraintlayout.widget.ConstraintLayout>